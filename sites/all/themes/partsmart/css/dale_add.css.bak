@charset "utf-8";
/* CSS Document */
.contex p.wholesale_price {
clear: both;
border-top: none;
padding: 5px 0;
line-height: 10px;
margin-left:90px;
}
#brand-items-list ul li{text-transform: lowercase;}
#brand-items-list li:first-letter{text-transform:uppercase;}
.home .banner-link a:link {
    color: #4C4B4B;
    text-decoration: none;
}
.home .banner-link a:visited {
    color: #4C4B4B;
    text-decoration: none;
}
.home .banner-link a:hover {
    color: #4C4B4B;
    text-decoration: underline;
}
.home .banner-link a:active {
    color: #4C4B4B;
    text-decoration: none;
}